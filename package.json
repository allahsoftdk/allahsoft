{
  "name": "allahsoft",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www.js",
    "debug": "npm install && nodemon -r dotenv/config --legacy-watch -P 1000 ./bin/www.js",
    "test": "dotenv -e .env.test npx prisma migrate dev && cross-env NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test jest --runInBand",
    "test:watch": "docker exec -it backend-dev sh -c \"dotenv -e .env.test npx prisma migrate dev\" dotenv -e .env.test && cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch --runInBand",
    "migrations": "docker exec -it backend-dev sh -c \"dotenv -e .env npx prisma migrate dev\""
  },
  "dependencies": {
    "@prisma/client": "^4.10.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.20",
    "scrypt-js": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^27.5.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/parser": "^5.51.0",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "prisma": "^4.10.0",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
